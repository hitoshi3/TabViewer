{
    "manifest_version": 2,
    "name": "List Tabs",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "icon.png"
    },

    "background": {
        "scripts": [
            "jquery.js",
            "background.js"
        ],
        "persistent": false
    },

    "permissions": [
        "tabs"
    ]

}